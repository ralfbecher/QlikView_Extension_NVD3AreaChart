<?xml version="1.0" encoding="utf-8" ?>
<ExtensionObject Path="NVD3AreaChart" 
                 Label="NV D3 Area Chart" 
				 Description="D3 Area Chart Example" 
 				 PageWidth="3"
				 PageHeight="2000"
				 Type="Object" >
	<Dimension Label="Key" Initial="" TargetName="Key" OtherCounted="6" />
	<Dimension Label="x-Axis" Initial="" TargetName="x-Axis" />
	<Measurement Label="y-Axis" Initial="" TargetName="y-Axis" />
    <Text Label="x-Axis d3 format (date)" Type="text" Expression="" />
    <Text Label="y-Axis d3 format (number)" Type="text" Expression="" />
    <Text Label="Show Others" Type="checkbox" Expression="1" />
	<Initiate Name="Caption.Text" value="NV D3 Area Chart" />
    <Initiate Name="Chart.Dimension.0.Field" Value="YourDimensionField" />
    <Initiate Name="Chart.Dimension.1.Field" Value="=Num(YourDateField)" />
    <Initiate Name="Chart.Expression.0.0.Definition" Value="=Sum(YourMeasureField)" />
</ExtensionObject>