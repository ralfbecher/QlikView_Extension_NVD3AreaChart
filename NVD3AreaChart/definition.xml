<?xml version="1.0" encoding="utf-8" ?>
<ExtensionObject Path="NVD3AreaChart" 
                 Label="NV D3 Area Chart" 
				 Description="D3 Area Chart Example" 
 				 PageWidth="3"
				 PageHeight="2000"
				 Type="Object" >
	<Dimension Label="Key" Initial="YourDimensionField" TargetName="Key" OtherCounted="6" />
